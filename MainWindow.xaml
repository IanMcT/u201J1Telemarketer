<Window x:Class="u2J1Telemarketer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:u2J1Telemarketer"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" FontSize="24">
    <StackPanel>
        <TextBox
            x:Name="txtInput"
            TextWrapping="Wrap"
            AcceptsReturn="True"
            Height="125" />
        <Button
            x:Name="btnRun"
            Content="Click to run"
            Click="btnRun_Click" />
        <Label
            x:Name="lblOutput"
            />

    </StackPanel>
</Window>
